   "cell_type": "code",
   "execution_count": 13,
   "id": "370c2cad-0cb9-40fd-ae0f-43236147de49",
   "metadata": {},
   "outputs": [],
   "source": [
    "# based on addicted score -create addiction level column(catrgorize <=3 -low, <=6 -medium, High)\n",
    "\n",
    "def addiction_level(score):\n",
    "    if score <= 3:\n",
    "        return \"Low\"\n",
    "    elif score <= 6:\n",
    "        return \"Medium\"\n",
    "    else:\n",
    "        return \"High\"\n",
    "\n",
    "df[\"Addiction_Level\"] = df[\"Addicted_Score\"].apply(addiction_level)"
   ]
  },
